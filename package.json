{
  "name": "vite-plugin-decaffeinate",
  "version": "0.1.0",
  "description": "A Vite plugin for your CoffeeScript projects",
  "license": "MIT",
  "author": "Jan T. Sott",
  "repository": {
    "type": "git",
    "url": "https://github.com/idleberg/vite-plugin-decaffeinate"
  },
  "main": "./dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^16.0.0 || 18.0.0 || >=22.0.0"
  },
  "files": [
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint:code": "biome check --config-path=biome.jsonc --no-errors-on-unmatched --vcs-enabled=true --vcs-use-ignore-file=true",
    "lint:deps": "knip --production",
    "lint": "concurrently --prefix '{name}' -c 'blue,green' 'npm:lint:*'",
    "prepack": "npm run build",
    "publish:jsr": "deno publish",
    "publish:npm": "np --no-yarn --any-branch",
    "release": "np --no-yarn --any-branch",
    "test": "uvu tests --ignore \"expected\" --ignore \"fixtures\" --ignore \"dist\" --ignore \"helper.mjs\""
  },
  "keywords": [
    "vite-plugin",
    "decaffeinate",
    "coffeescript",
    "coffee-script"
  ],
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@total-typescript/tsconfig": "^1.0.4",
    "@types/node": "^18",
    "concurrently": "^9.1.2",
    "decaffeinate": "^8.1.4",
    "glob": "^11.0.0",
    "husky": "^9.1.7",
    "knip": "^5.41.1",
    "lefthook": "^1.11.12",
    "nano-staged": "^0.8.0",
    "np": "^10.1.0",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "uvu": "^0.5.6",
    "vite": "^6.0.7"
  },
  "peerDependencies": {
    "decaffeinate": ">=8.0.0",
    "vite": "^5.0.0 || >=6.0.0"
  },
  "nano-staged": {
    "*.ts": [
      "eslint --cache --fix",
      "prettier --cache --write"
    ]
  }
}
